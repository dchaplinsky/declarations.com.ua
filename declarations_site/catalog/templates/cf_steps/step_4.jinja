{% if not step_data['empty'] %}
    <div class="table-responsive">
        <table class="table table-striped table-bordered card-table border corner-round dataTable">
            <thead>
                <tr>
                    <th class="doc-num-row-col">№</th>
                    <th>{{ _('table_header_common_info') }}</th>
                    <th>{{ _('table_header_type_info') }}</th>
                    <th>{{ _('table_header_costDate_info') }}</th>
                    <th>{{ _('table_header_costAssessment_info') }}</th>
                    <th>{{ _('table_header_sources_info') }}</th>
                </tr>
            </thead>
            <tbody>
            {% for item in step_data %}
                {% set data_item = step_data[item] %}
                <tr>
                    <td>{# № #}
                        {{ loop.index }}
                    </td>
                    <td>
                        <div class="row mb-2 mr-1">
                            <div class="col-12">
                                <span class="color-1">{{ _('objectType_step5') }}</span>
                                <span>{{ display_val(data_item.objectType) }}</span>
                            </div>
                            {% if otherObjectType %}
                                <div class="col-12">
                                    <span class="color-1">{{ _('otherObjectType_step5') }}</span>
                                    <span>{{ display_val(data_item.otherObjectType) }}</span>
                                </div>
                            {% endif %}
                            <div class="col-12">
                                <span class="color-1">{{ _('owningDate') }}</span>
                                <span>{{ display_val(data_item.owningDate) }}</span>
                            </div>
                            <div class="col-12">
                                <span class="color-1">{{ _('vinCode') }}</span>
                                <span>{{ display_val(data_item.vinCode) }}</span>
                            </div>
                        </div>
                    </td>
                    <td>
                        <div class="row mb-2 mr-1">
                            <div class="col-12">
                                <span class="color-1">{{ _('brand') }}</span>
                                <span>{{ display_val(data_item.brand) }}</span>
                            </div>
                            <div class="col-12">
                                <span class="color-1">{{ _('model') }}</span>
                                <span>{{ display_val(data_item.model) }}</span>
                            </div>
                            <div class="col-12">
                                <span class="color-1">{{ _('graduationYear') }}</span>
                                <span>{{ display_val(data_item.graduationYear) }}</span>
                            </div>
                        </div>
                    </td>
                    <td>
                        {{ display_val(data_item.costDate) }}
                    </td>
                    <td>
                        {{ display_val(data_item.costLast) }}
                    </td>
                    <td>{# sourses #}
                        {% for source in data_item.sources %}
                            <div class="row mb-2 mr-1">
                                <div class="col-12">
                                    <span class="color-1">{{ _('sourceType') }}</span>
                                    <span>{{ display_val(source.sourceType) }}</span>
                                </div>
                                <div class="col-12">
                                    <span class="color-1">{{ _('ua_lastname') }}</span>
                                    <span>{{ display_val(source.ua_lastname) }}</span>
                                </div>
                                <div class="col-12">
                                    <span class="color-1">{{ _('ua_firstname') }}</span>
                                    <span>{{ display_val(source.ua_firstname) }}</span>
                                </div>
                                <div class="col-12">
                                    <span class="color-1">{{ _('ua_middlename') }}</span>
                                    <span>{{ display_val(source.ua_middlename) }}</span>
                                </div>
                                <div class="col-12">
                                    <span class="color-1">{{ _('ua_birthday') }}</span>
                                    <span>{{ display_val(source.ua_birthday) }}</span>
                                </div>
                                <div class="col-12">
                                    <span class="color-1">{{ _('ua_taxNumber') }}</span>
                                    <span>{{ display_val(source.ua_taxNumber) }}</span>
                                </div>
                                <div class="col-12">
                                    <span class="color-1">{{ _('ua_regAddressFull') }}</span>
                                    <span>{{ display_val(source.ua_regAddressFull) }}</span>
                                </div>
                                <div class="col-12">
                                    <span class="color-1">{{ _('ua_livingAddressFull') }}</span>
                                    <span>{{ display_val(source.ua_livingAddressFull) }}</span>
                                </div>
                            </div>
                        {% endfor %}
                    </td>
                </tr>
            {% endfor %}
            </tbody>
        </table>
    </div>
{% else %}
    {{ step_data['empty'] }}
{% endif %}